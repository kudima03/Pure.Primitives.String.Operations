<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net7.0;net8.0;net9.0;net10.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsAotCompatible>true</IsAotCompatible>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.ILLink.Tasks" Version="9.0.10" />
    <PackageReference Include="Pure.Primitives" Version="3.5.0" />
    <PackageReference Include="Pure.Primitives.Abstractions" Version="4.2.0" />
  </ItemGroup>
  <PropertyGroup>
    <PackageId>Pure.Primitives.String.Operations</PackageId>
    <RootNamespace>Pure.Primitives.String.Operations</RootNamespace>
    <AssemblyName>Pure.Primitives.String.Operations</AssemblyName>
    <Authors>Dmitry Kurochkin</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/kudima03/Pure.Primitives.String.Operations</PackageProjectUrl>
  </PropertyGroup>
</Project>
